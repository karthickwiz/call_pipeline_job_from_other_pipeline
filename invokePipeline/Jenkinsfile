pipeline {
    agent any
     stages {
        stage('Initialize') {
            steps {
                echo 'setting...'   
                 bat 'set'
            }
        }
       stage('build') {
          steps {                                
             bat 'echo building....'
             bat 'gradle build'
               bat 'echo builded....'    
            
          }
       }
         stage ("invoking clsd pipeline") {
            steps {
                echo "clsddddddddddddddddddddddddddd"
                build job: 'clsd'
            }
        }
         
          
    }
    post { 
        always { 
           
             echo 'Done!'
        }
    }
}
